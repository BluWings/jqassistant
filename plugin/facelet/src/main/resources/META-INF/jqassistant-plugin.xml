<jqa-plugin:jqassistant-plugin xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.0"
                               name="Facelet">
    <description>Provides a scanner for JSF template files.</description>
    <model>
        <class>com.buschmais.jqassistant.plugin.facelet.api.model.JsfFaceletDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.facelet.api.model.JsfDescriptor</class>
    </model>
    <scanner>
        <class>com.buschmais.jqassistant.plugin.facelet.impl.scanner.JsfFaceletScannerPlugin</class>
    </scanner>
</jqa-plugin:jqassistant-plugin>